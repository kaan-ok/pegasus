  "version": "1.0.0",
  "description": "A modern e-commerce website for Pegasus Power energy drinks",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.js",
    "format": "prettier --write \"**/*.{js,ts,html,css,json}\""
  },
  "keywords": [
    "e-commerce",
    "energy-drinks",
    "shop",
    "tailwindcss",
    "javascript"
  ],
  "author": "Pegasus Power",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "eslint": "^9.25.1",
