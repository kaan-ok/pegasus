    "prettier": "^3.5.3",
    "typescript": "~5.7.2",
    "vite": "^6.3.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
